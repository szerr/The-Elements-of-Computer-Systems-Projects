/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Out16 {
    IN in[16];
    OUT out[16];

    PARTS:
    // Put your code here:
    And16(a=in, b[0..15]=true, out=out);
}